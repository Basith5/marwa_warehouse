// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id              Int       @id @default(autoincrement())
  email           String
  password        String
}

model products {
  id              Int       @id @default(autoincrement())
  productName     String
  quantity        Int
  mrp             Int
  discount        Int
  netRate         Int
  add             Int
  saleRate        Int
  category        String
}